rules_version = '2';

service cloud.firestore {
  match /databases/{database}/documents {
    // Allow all read/write access for development and migration
    // WARNING: These rules allow anyone to read/write. Replace with proper auth before production!
    match /{document=**} {
      // else all client requests to your database will be denied until you
      // update your rules.
      allow read, write: if request.time < timestamp.date(2026, 2, 18);
    }
  }
}
